    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "Telvanni",
    "Text": "You belong to House Telvanni, do you not? Where do your true loyalties lie? House Telvanni opposes our every effort at modernizing the way magic is practiced here in Vvardenfell.",
    "Filters": [],
    "Script": []
  },
  "1879277768809430": {
    "Prev": "1372630516203504528",
    "Next": "3150519672119583740",
