  "404511631560710147": {
    "Prev": "25623826767188145",
    "Next": "15299226772113513580",
    "Type": "Topic",
    "Disposition": 50,
    "SpeakerRank": -1,
    "SpeakerSex": "Any",
    "PlayerRank": -1,
    "SpeakerId": "yakum hairshashishi",
    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "There's bad magic in the dust from Red Mountain. The Sharmat Dagoth Ur sends bad dreams in the dust. The people can't sleep, act crazy. When the blight storms come, soul sickness comes -- both together. When I lived in the ashlands, I got no sleep had bad dreams, was all the time tired. Now that I'm here in Pelagiad, I sleep fine.",
    "Filters": [],
    "Script": []
  },
  "15299226772113513580": {
    "Prev": "404511631560710147",
    "Next": "2041312069520521327",
    "Type": "Topic",
    "Disposition": 0,
    "SpeakerRank": -1,
    "SpeakerSex": "Any",
