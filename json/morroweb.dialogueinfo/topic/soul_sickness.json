    "Script": []
  },
  "317433278116041054": {
    "Prev": "157665906528415585",
    "Next": "25623826767188145",
    "Type": "Topic",
    "Disposition": 0,
    "SpeakerRank": -1,
    "SpeakerSex": "Any",
    "PlayerRank": -1,
    "SpeakerId": "",
    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "Ashlanders",
    "SpeakerCell": "Ahemmusa Camp",
    "PlayerFaction": "",
    "Text": "The Devil Dagoth sends bad dreams in the red dust. People don't sleep, go mad. Blight storms come, and soul sicknesses come, all together. Men get no sleep, get so sick, so tired. Very bad.",
    "Filters": [],
    "Script": []
  },
  "25623826767188145": {
    "Prev": "317433278116041054",
    "Next": "404511631560710147",
