    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "Ashlanders",
    "SpeakerCell": "Ahemmusa Camp",
    "PlayerFaction": "",
    "Text": "Now that the Devil Dagoth is dead, there should be an end to the dark dreams.",
    "Filters": [
      {
        "Type": "Journal",
        "Func": "JournalType",
        "Compare": "GreaterEqual",
        "Id": "C3_DestroyDagoth",
        "Value": 50.0
