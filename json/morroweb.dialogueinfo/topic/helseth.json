    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "He is my son. He is also the King of Morrowind.",
    "Filters": [],
    "Script": []
  },
