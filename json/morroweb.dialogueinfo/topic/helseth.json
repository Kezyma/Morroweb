    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "I'm surprised the king has been killed. He seemed like the careful sort.",
    "Filters": [
      {
        "Type": "Dead",
        "Func": "DeadType",
        "Compare": "Greater",
        "Id": "King Hlaalu Helseth",
        "Value": 0.0
