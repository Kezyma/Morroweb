    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "I am King Hlaalu Helseth, ruler of all Morrowind.",
    "Filters": [],
    "Script": []
  },
  "32119362846714488": {
