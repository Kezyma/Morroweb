    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "My son is dead. My only hope is that his murderer will be brought to justice.",
    "Filters": [
      {
        "Type": "Dead",
        "Func": "DeadType",
        "Compare": "Greater",
        "Id": "King Hlaalu Helseth",
        "Value": 0.0
      }
    ],
    "Script": []
  },
