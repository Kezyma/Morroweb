    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "I believe that business is complete. Thank you for your help.",
    "Filters": [
      {
        "Type": "Journal",
        "Func": "JournalType",
        "Compare": "GreaterEqual",
        "Id": "BM_Morale",
