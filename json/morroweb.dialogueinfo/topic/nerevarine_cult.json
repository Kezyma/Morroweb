    "SpeakerId": "",
    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "Imperial Legion",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "So how does it feel to have a cult worshipping you, %PCName?",
    "Filters": [
      {
        "Type": "NotLocal",
        "Func": "VariableCompare",
        "Compare": "Equal",
        "Id": "nolore",
        "Value": 0.0
      },
      {
        "Type": "Journal",
        "Func": "JournalType",
        "Compare": "GreaterEqual",
