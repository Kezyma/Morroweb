    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "That is not a subject for idle conversation.",
    "Filters": [],
    "Script": []
  },
  "2878618358233196267": {
    "Prev": "1481620928894511200",
    "Next": "2248623544292614292",
    "Type": "Topic",
    "Disposition": 0,
    "SpeakerRank": -1,
    "SpeakerSex": "Any",
