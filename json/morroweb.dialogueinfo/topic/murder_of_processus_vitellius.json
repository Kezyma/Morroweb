    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "Good to see you were able to recover the Emperor's coin. I appreciate that kind of honesty. Since you've proven trustworthy, here's something you might be interested in. I'd like to see the murderer of Processus punished. Killing an Emperor's tax collector is punishable by death. Find his murderer, bring him to justice, and the Census and Excise office will pay you 500 septims.",
    "Filters": [
      {
        "Type": "Journal",
        "Func": "JournalType",
        "Compare": "Equal",
        "Id": "MV_DeadTaxman",
        "Value": 45.0
      },
      {
        "Type": "Item",
        "Func": "ItemType",
        "Compare": "GreaterEqual",
        "Id": "Gold_001",
        "Value": 200.0
      }
    ],
    "Script": [
      "Journal \"MV_DeadTaxman\" 46",
      "Player->RemoveItem \"Gold_001\" 200",
      "AddItem \"Gold_001\" 200",
      "Journal \"MV_DeadTaxman\" 48"
    ]
  },
  "1932910257292029503": {
    "Prev": "4145450230103435",
    "Next": "1023211710410632024",
    "Type": "Topic",
    "Disposition": 0,
    "SpeakerRank": -1,
    "SpeakerSex": "Any",
