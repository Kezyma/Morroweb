        "Value": 200.0
      },
      {
        "Type": "Journal",
        "Func": "JournalType",
        "Compare": "Less",
        "Id": "MV_DeadTaxman",
        "Value": 20.0
      }
    ],
    "Script": [
      "Journal \"MV_DeadTaxman\" 20",
      "Choice \"Yes, I found 200 septims on his body.\" 1 \"No, he didn't have anything on him.\" 2"
    ]
  },
