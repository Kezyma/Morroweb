    ]
  },
  "2415317921348916706": {
    "Prev": "1023211710410632024",
    "Next": "4412256843171812107",
    "Type": "Topic",
    "Disposition": 0,
    "SpeakerRank": -1,
    "SpeakerSex": "Any",
    "PlayerRank": -1,
    "SpeakerId": "chargen class",
    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "You've already spent it? That's not good to hear. That was Imperial tax money, after all. You'd do well to get that money back and return it to me.",
    "Filters": [
      {
        "Type": "Function",
        "Func": "Choice",
        "Compare": "Equal",
        "Id": "",
        "Value": 3.0
      }
    ],
