    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "No? It figures, I suppose. Well, thank you for letting me know what had happened.",
    "Filters": [
      {
        "Type": "Function",
        "Func": "Choice",
        "Compare": "Equal",
        "Id": "",
        "Value": 2.0
      }
    ],
