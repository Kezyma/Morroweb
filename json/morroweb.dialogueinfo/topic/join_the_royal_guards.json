  "3161488311799414056": {
    "Prev": "24114217712821316792",
    "Next": "546847141757910745",
    "Type": "Topic",
    "Disposition": 0,
    "SpeakerRank": -1,
    "SpeakerSex": "Any",
    "PlayerRank": -1,
    "SpeakerId": "Drusus Gratus",
    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "My father had the honor to serve King Helseth and Queen Barenziah in Wayrest. My family has served the Queen for three generations. I don't know your qualifications, but I presume you've satisfied Tienius Delitian, or we wouldn't be talking.",
    "Filters": [
      {
        "Type": "Journal",
        "Func": "JournalType",
        "Compare": "GreaterEqual",
        "Id": "TR07_Guard",
        "Value": 1.0
      }
    ],
    "Script": [
      "Choice \"Ask about King Llethan.\" 1 \"Ask about King Helseth.\" 2 \"Mention your Hlaalu connections.\" 3"
    ]
