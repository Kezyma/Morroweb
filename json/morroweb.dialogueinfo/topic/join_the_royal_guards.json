    "Next": "57534442667024103",
    "Type": "Topic",
    "Disposition": 0,
    "SpeakerRank": -1,
    "SpeakerSex": "Any",
    "PlayerRank": -1,
    "SpeakerId": "Alusannah",
    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "Tell Tienius Delitian, if you haven't already. It may not be important. But Delitian won't like it if you try to keep it secret.",
    "Filters": [
      {
        "Type": "Journal",
        "Func": "JournalType",
        "Compare": "GreaterEqual",
