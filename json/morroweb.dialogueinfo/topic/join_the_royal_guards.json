    "Next": "26585303952110923358",
    "Type": "Topic",
    "Disposition": 0,
    "SpeakerRank": -1,
    "SpeakerSex": "Any",
    "PlayerRank": -1,
    "SpeakerId": "Milvela Dralen",
    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "I'd rather not talk about it.",
    "Filters": [
      {
        "Type": "Journal",
