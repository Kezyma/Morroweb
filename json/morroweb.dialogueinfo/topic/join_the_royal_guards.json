    "SpeakerId": "Alusannah",
    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "Nine blessings for the old man. Pity. But we all come to it in the end.",
    "Filters": [
      {
        "Type": "Journal",
        "Func": "JournalType",
        "Compare": "GreaterEqual",
        "Id": "TR07_Guard",
        "Value": 1.0
      },
