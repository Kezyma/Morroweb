    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "Well then!  If that's all you think I am, then clearly we have nothing more to say to one another.",
    "Filters": [
      {
        "Type": "Function",
        "Func": "Choice",
        "Compare": "Equal",
        "Id": "",
        "Value": 4.0
      }
    ],
