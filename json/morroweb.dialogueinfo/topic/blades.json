    "SpeakerId": "caius cosades",
    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "We're spies. We're the Emperor's hidden eyes and ears in the provinces. We watch the Emperor's enemies. We look for opportunities. We make reports. And, when the Emperor commands, we obey. But we're only one of many guilds and factions in Morrowind. You should know a little about Morrowind politics before you get involved with any of these other groups.",
    "Filters": [
      {
        "Type": "Journal",
        "Func": "JournalType",
        "Compare": "GreaterEqual",
        "Id": "A1_1_FindSpymaster",
        "Value": 14.0
      }
    ],
    "Script": []
  },
