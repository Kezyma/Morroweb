    "Next": "1382723294641421474",
    "Type": "Journal",
    "Disposition": 100,
    "SpeakerRank": -1,
    "SpeakerSex": "Any",
    "PlayerRank": -1,
    "SpeakerId": "",
    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "Wind-Eye was pleased that the Ristaag was successful. He has taught me more of the Skaal magic in return for my efforts. I am now able to call a bear to aid me in times of need.",
    "Filters": [],
    "Script": []
  },
  "1382723294641421474": {
