    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "I have returned to Druscashti and told Volrina Quarra of the death of \"Lord Irarak.\" She was pleased, and will allow me to use clan services.",
    "Filters": [],
    "Script": []
  },
