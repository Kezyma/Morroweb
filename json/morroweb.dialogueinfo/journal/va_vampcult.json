    "SpeakerId": "",
    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "Irarak is dead. His cult will have to find a new way to spend their time and gold.",
    "Filters": [],
    "Script": []
  },
  "10565207852121112490": {
