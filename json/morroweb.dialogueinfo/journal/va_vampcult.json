    "SpeakerId": "",
    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "I have agreed to try and kill Irarak, rogue vampire and leader of his \"dark cult.\" Volrina Quarra has told me I am likely to find him near Gnisis.",
    "Filters": [],
    "Script": []
  },
  "92246382227901197": {
    "Prev": "11965181037321119",
    "Next": "21402184161265423862",
