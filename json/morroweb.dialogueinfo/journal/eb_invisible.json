    "SpeakerRace": "",
    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "I told Cassius Olcinius that Fevyn Ralen is dead. He'll never be visible again.",
    "Filters": [],
    "Script": []
  },
