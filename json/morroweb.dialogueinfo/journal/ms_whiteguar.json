    "SpeakerClass": "",
    "SpeakerFaction": "",
    "SpeakerCell": "",
    "PlayerFaction": "",
    "Text": "I have decided not to try and find the white guar from Urshamusa's vision.",
    "Filters": [],
    "Script": []
  },
